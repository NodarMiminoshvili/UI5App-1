{
  "_version": "1.58.0",
  "sap.app": {
    "id": "nodar.miminoshvili",
    "i18n": {
      "bundleUrl": "i18n/i18n.properties"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "type": "application",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "mainService": {
        "uri": "/V2/(S(g2nixij2iu2fndbjc2qzlj3f))/OData/OData.svc/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "handleValidation": true,
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.comp": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "nodar.miminoshvili.i18n.i18n",
          "async": true,
          "supportedLocales": [
            "en"
          ],
          "fallbackLocale": "en"
        }
      },
      "": {
        "dataSource": "mainService",
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "defaultBindingMode": "TwoWay",
          "useBatch": false
        }
      }
    },
    "rootView": {
      "viewName": "nodar.miminoshvili.view.App",
      "type": "XML",
      "id": "app"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "type": "View",
        "viewType": "XML",
        "path": "nodar.miminoshvili.view",
        "controlId": "app",
        "controlAggregation": "pages"
      },
      "routes": [
        {
          "pattern": "",
          "name": "ProductList",
          "target": "productList"
        },
        {
          "pattern": "product/{productId}",
          "name": "ProductDetails",
          "target": "product"
        }
      ],
      "targets": {
        "productList": {
          "name": "ProductList"
        },
        "product": {
          "name": "ProductDetails"
        }
      }
    }
  }
}